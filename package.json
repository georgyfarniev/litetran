{
  "name": "litetran",
  "productName": "litetran",
  "version": "1.0.0",
  "description": "LiteTran",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run dev:start",
    "dev:start": "concurrently \"npm run start:react\" \"npm run start\"",
    "start:react": "cross-env BROWSER=none react-scripts start",
    "build:react": "react-scripts build",
    "start": "tsc -p tsconfig.electron.json && electron-forge start --app-path dist/electron/index.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Georgy Farniev",
    "email": "gfarniev@icloud.com"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "litetran_ng"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-deb": "^6.0.0-beta.51",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "electron": "8.2.5",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "typescript": "^3.8.3",
    "@fluentui/react": "^7.112.0",
    "@types/axios": "^0.14.0",
    "axios": "^0.19.2",
    "@use-hooks/axios": "^1.3.1"
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "robotjs": "^0.6.0"
  }
}
